{
    "version": 4,
    "configurations": [
        {
            "name": "papermario",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/include",
                    "${workspaceFolder}/src"
                ]
            },
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/ver/us/build/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/assets/us",
            ],
            "defines": [
                "F3DEX_GBI_2",
                "_LANGUAGE_C",
                "SCRIPT(...)={}",
                "VERSION=us"
            ],
            "cStandard": "c89",
            "cppStandard": "c++17"
        }
    ]
}
